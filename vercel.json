{
  "version": 2,
  "builds": [
    {
      "src": "webapp/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "webapp/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "webapp/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
